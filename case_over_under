SELECT CASE WHEN Hours Scheduled: % Billable [no pto] = Billability Target KLM THEN 1, 
WHEN Hours Scheduled: % Billable [no pto]BETWEEN (SELECT SUM(Billability Target KLM - 0.05)) AND (SELECT SUM(Billability Target KLM + 0.05)) THEN 2, 
WHEN Hours Scheduled: % Billable [no pto] < (SELECT SUM(Billability Target KLM - 0.05)) THEN 3, 
WHEN Hours Scheduled: % Billable [no pto] > (SELECT SUM(Billability Target KLM + 0.05)) THEN 4 ELSE 0 END

[=0][color=dddbdd]No Activity;
 [=1] ⭐;
 [=2][color=23ef94] On Target;
 [=3][color=efcd23] UNDER;
 [=4][color=ef23c2] OVER
